## The function is currently defined as
function (x, a, d = 0.1) 
{
    df2_fun <- function(i) f2(i - d, a) - f2(i + d, a)
    dhalf = df2_fun(0)
    dx = df2_fun(x)
    return(dx/dhalf)
  }
}
