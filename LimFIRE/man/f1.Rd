## The function is currently defined as
function (x, a, b, force_zero = FALSE) 
{
    f1A <- function(xi) 1/(1 + a * exp(-b * xi))
    f1 = f1A(x)
    if (force_zero) {
        f0 = f1A(0)
        f1 = (f1 - f0)/(1 - f0)
        f1[is.na(f1)] = 0
    }
    return(f1)
  }
}
